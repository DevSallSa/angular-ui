{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "storybook-angular": {
      "projectType": "application",
      "schematics": {},
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/storybook-angular",
            "index": "projects/storybook-angular-examples/src/index.html",
            "main": "projects/storybook-angular-examples/src/main.ts",
            "polyfills": "projects/storybook-angular-examples/src/polyfills.ts",
            "tsConfig": "tsconfig.json",
            "aot": true,
            "styles": [
              "projects/core-components-angular-lib/global.scss",
              "node_modules/@villemontreal/boite-outils4-web/dist/css/styles.min.css",
              "projects/storybook-angular/src/styles.css"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "storybook-angular:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "storybook-angular:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "storybook-angular:build"
          }
        },
        "test": {
          "builder": "@angular-builders/jest:run"
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "lintFilePatterns": ["projects/storybook-angular/src/**/*.ts"]
          }
        },
        "storybook": {
          "builder": "@storybook/angular:start-storybook",
          "options": {
            "browserTarget": "angular-cli:build",
            "port": 4400
          }
        },
        "build-storybook": {
          "builder": "@storybook/angular:build-storybook",
          "options": {
            "browserTarget": "angular-cli:build"
          }
        }
      }
    },
    "core-components-angular-lib": {
      "projectType": "library",
      "root": "projects/core-components-angular-lib",
      "sourceRoot": "projects/core-components-angular-lib/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/core-components-angular-lib/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/core-components-angular-lib/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/core-components-angular-lib/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/core-components-angular-lib/src/test.ts",
            "tsConfig": "projects/core-components-angular-lib/tsconfig.spec.json",
            "karmaConfig": "projects/core-components-angular-lib/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "lintFilePatterns": [
              "projects/core-components-angular-lib/**/*.ts",
              "projects/core-components-angular-lib/**/*.html"
            ]
          }
        }
      }
    },
    "storybook-angular-examples": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        },
        "@schematics/angular:application": {
          "strict": true
        }
      },
      "root": "projects/storybook-angular-examples",
      "sourceRoot": "projects/storybook-angular-examples/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/storybook-angular-examples",
            "index": "projects/storybook-angular-examples/src/index.html",
            "main": "projects/storybook-angular-examples/src/main.ts",
            "polyfills": "projects/storybook-angular-examples/src/polyfills.ts",
            "tsConfig": "projects/storybook-angular-examples/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "projects/storybook-angular-examples/src/favicon.ico",
              "projects/storybook-angular-examples/src/assets"
            ],
            "styles": ["projects/storybook-angular-examples/src/styles.scss"],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "projects/storybook-angular-examples/src/environments/environment.ts",
                  "with": "projects/storybook-angular-examples/src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "storybook-angular-examples:build:production"
            },
            "development": {
              "browserTarget": "storybook-angular-examples:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "storybook-angular-examples:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/storybook-angular-examples/src/test.ts",
            "polyfills": "projects/storybook-angular-examples/src/polyfills.ts",
            "tsConfig": "projects/storybook-angular-examples/tsconfig.spec.json",
            "karmaConfig": "projects/storybook-angular-examples/karma.conf.js",
            "inlineStyleLanguage": "scss",
            "assets": [
              "projects/storybook-angular-examples/src/favicon.ico",
              "projects/storybook-angular-examples/src/assets"
            ],
            "styles": ["projects/storybook-angular-examples/src/styles.scss"],
            "scripts": []
          }
        }
      }
    }
  },
  "defaultProject": "storybook-angular",
  "cli": {
    "defaultCollection": "@angular-eslint/schematics"
  }
}
